{
  "name": "wolfui",
  "version": "0.0.1",
  "description": "Configurable react components",
  "homepage": "https://github.com/wolfui/wolfui#readme",
  "main": "/src/index.js",
  "scripts": {
    "build:dist": "npm run clean:dist && node_modules/webpack/bin/webpack.js",
    "clean:dist": "rm -rf dist",
    "docs:serve": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./config/webpack.docs.config.js",
    "test": "mocha --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolfui/wolfui.git"
  },
  "keywords": [
    "react",
    "components",
    "user interface"
  ],
  "author": "MayStar Consulting, LLC.",
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "engines": {
    "node": ">=7.4.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolfui/wolfui/issues"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  }
}
